        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/tv1"
            android:text="그림1"
            android:textSize="15sp"
            android:layout_gravity="center_vertical"
            android:layout_span="2"/>
        <RatingBar
            android:id="@+id/rbar1"
            style="@style/Widget.AppCompat.RatingBar.Small"/>
    </TableRow>
    <TableRow
